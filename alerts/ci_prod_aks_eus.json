                        "resolveConfiguration": {
                            "autoResolved": true,
                            "timeToResolve": "PT10M"
                        },
                        "actions": [
                            {
                                "ActionProperties": {
                                    "Icm.Enabled": "True"
                                }
                            }
                        ]
                    },
