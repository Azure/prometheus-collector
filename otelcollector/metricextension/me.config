{
   "otlp":{
      "endpoints":[
         "127.0.0.1:55680"
      ],
      "resourceAttributes":[
        "cluster",
        "job",
        "instance"
      ],
      "honorResourceAttributes": true
   },
   "enableParallelization": true,
   "maxPublicationAttemptsPerMinute":12,
   "enableTwoStagePublication": true,
   "maxPublicationPackageSizeInBytes":50000000,
   "maxPublicationBytesPerMinute":200000000,
   "maxPublicationMetricsPerMinute":10000000,
   "maxAggregationQueueSize":6000000,
   "maxNumberOfRawEventsPerCycle":5000000,
   "maxAggregationTimeoutPerCycleInSec":40,
   "aggregationProcessingParallelizationFactor":10,
   "compressMetricData": true,
   "internalMetricProductionLevel":2,
   "enableInternalMetrics2":true,
   "transformRules" : [
      {
        "condition": {
            "type": "metric_key",
            "allowed_names": ["kubelet"]
        },
        "operations": [
            {
                "type": "log"
            }
        ]
      }
    ]
}