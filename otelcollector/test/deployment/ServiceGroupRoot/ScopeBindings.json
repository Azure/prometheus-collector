{
  "$schema": "http://schema.express.azure.com/schemas/2015-01-01-alpha/ScopeBindings.json",
  "contentVersion": "1.0.0.0",
    "scopeBindings": [
        {
            "scopeTagName": "Common",
            "bindings": [
                {
                    "find": "$_ALLOWED_REGIONS_$",
                    "replaceWith": "$config(allowedRegions)"
                }
                {
                    "find": "$_ENV_$",
                    "replaceWith": "$config(environment)"
                },                
                {
                    "find": "$_SUBSCRIPTION_ID_$",
                    "replaceWith": "$config(subscriptionId)"
                },   
                {
                    "find": "$_RESOURCE_GROUP_NAME_$",
                    "replaceWith": "$azureResourceGroup()"
                },                             
                {
                    "find": "$_LOG_STORAGE_ACCOUNT_NAME_$",
                    "replaceWith": "stshellextlogs-$azureResourceGroup()"
                },               
                {
                    "find": "$_VNET_NAME_$",
                    "replaceWith": "$config(vnetNameBase)$azureResourceGroup()"
                },
                {
                    "find": "$_NAT_GATEWAY_NAME_$",
                    "replaceWith": "ipAddrNatGateway-$azureResourceGroup()"
                },                
                {
                    "find": "$_SHELL_SUBNET_NAME_$",
                    "replaceWith": "$config(shellExtSubnetNameBase)$azureResourceGroup()"
                },
                {
                    "find": "$_CLIENT_ID_$",
                    "replaceWith": "$config(clientId)"
                },
                {
                    "find": "$_PRINCIPAL_ID_$",
                    "replaceWith": "$config(principalId)"
                },
                {
                    "find": "$_MANAGED_IDENTITY_NAME_$",
                    "replaceWith": "$config(managedIdentityName)"
                },
                {
                    "find": "$_MANAGED_IDENTITY_ID_$",
                    "replaceWith": "/subscriptions/$azureSubscriptionId()/resourceGroups/$azureResourceGroup()/providers/Microsoft.ManagedIdentity/userAssignedIdentities/$config(managedIdentityName)"
                },
                {
                    "find": "$_AKS_NAME_$",
                    "replaceWith": "aks-cluster-$azureResourceGroup()"
                },
                {
                    "find": "$_AMW_NAME_$",
                    "replaceWith": "aks-cluster-$azureResourceGroup()-amw"
                },
                {
                    "find": "$_SHELL_SUBNET_ID_$",
                    "replaceWith": "/subscriptions/$azureSubscriptionId()/resourceGroups/$azureResourceGroup()/providers/Microsoft.Network/virtualNetworks/$config(vnetNameBase)$azureResourceGroup()/subnets/$config(shellExtSubnetNameBase)$azureResourceGroup()"
                },
                {
                    "find": "$_AGENT_POOL_VM_SIZE_$",
                    "replaceWith": "$config(agentPoolVmSize)"
                },
                {
                    "find": "$_MARINER_VM_SIZE_$",
                    "replaceWith": "$config(marinerVmSize)"
                },                        
                {
                    "find": "$_ARM64_VM_SIZE_$",
                    "replaceWith": "$config(arm64VmSize)"
                },                 
                {
                    "find": "$_MARINERARM64_VM_SIZE_$",
                    "replaceWith": "$config(marinerarm64VmSize)"
                },                
                {
                    "find": "$_WIN22_VM_SIZE_$",
                    "replaceWith": "$config(win22VmSize)"
                },          
                {
                    "find": "$_WIN19_VM_SIZE_$",
                    "replaceWith": "$config(win19VmSize)"
                }
            ]
        }     
    ]
}
