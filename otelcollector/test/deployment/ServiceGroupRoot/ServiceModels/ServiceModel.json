{
  "$schema": "https://ev2schema.azure.net/schemas/2020-04-01/RegionAgnosticServiceModel.json",
  "contentVersion": "1.0.0",
  "serviceMetadata": {
        "serviceIdentifier": "335c5688-a203-441d-9dc7-89f7b874a113",
        "serviceGroup": "Microsoft.Azure.PrometheusCollector.GinkgoE2E.Tests",
        "displayName": "PrometheusCollector GinkgoE2E Tests",
        "tenantId": "$config(tenant)",
        "environment": "$config(environment)"
  },  
  "serviceResourceGroupDefinitions": [
    {
      "name": "Run.PromTests",
      "subscriptionKey": "prom-test-$location()",
      "azureResourceGroupName": "prom-test-$location()",
      "stamps": {
        "Count": 1
      },
      "scopeTags": [    
        {
          "name": "Common"
        }
      ],
      "serviceResourceDefinitions": [
        {
          "name": "FetchMSIClientId",
          "composedOf": {
            "arm": {
              "templatePath": "Templates/fetch-msi-clientid.Template.json",
              "parametersPath": "Parameters/fetch-msi-clientid.parameters.json"
            }
          }          
        },        
        {
          "name": "ClusterRoleAssignments",
          "composedOf": {
            "arm": {
              "templatePath": "Templates/RoleAssignments.Template.json",
              "parametersPath": "Parameters/RoleAssignments.Parameters.json"
            }
          }
        },      
        {
          "name": "PromCollectorTestsShellExtension",
          "composedOf": {
            "extension": {
              "rolloutParametersPath": "RolloutParameters/PromCollectorTests.ShellExtension.RolloutParameters.json",
              "shell": [
                {
                  "type": "runScriptShell",
                  "properties": {
                      "imageName": "adm-mariner-20-l",
                      "imageVersion": "v4"
                  }                  
                }
              ]
            }            
          }
        }
      ]
    }
  ]
}