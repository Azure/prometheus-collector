                        "for": "PT3M",
                        "labels": {
                            "cluster": "ci-prod-aks-eus"
                        },
                        "annotations": {
                            "description": "up metric is not flowing for target = kubelet in cluster ci-prod-aks-eus"
                        },
                        "severity": 4,
                        "resolveConfiguration": {
                            "autoResolved": true,
                            "timeToResolve": "PT10M"
