                "rules": [
                    {
                        "alert": "Amd64 metric missing in cluster ci-prod-aks-eus",
                        "expression": "absent(node_uname_info{cluster=\"ci-prod-aks-eus\", machine=\"x86_64\"}) == 1 or node_uname_info{cluster=\"ci-prod-aks-eus\", machine=\"x86_64\"} == 0",
