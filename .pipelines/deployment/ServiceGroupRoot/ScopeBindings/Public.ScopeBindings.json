{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [       
      {
          "scopeTagName": "Global",
          "bindings": [        
              {
                  "find": "__ACR_REGISTRY__",
                  "replaceWith": "$(ACRRegistry)"
              },            
              {
                  "find": "__PROD_ACR_AGENT_REPOSITORY__",
                  "replaceWith": "$(ProdACRAgentRepository)"
              },
              {
                "find": "__PROD_ACR_CHART_REPOSITORY__",
                "replaceWith": "$(ProdACRChartRepository)"
              },
              {
                "find": "__MANAGED_IDENTITY__",
                "replaceWith": "$(ManagedIdentity)"
              },
              {
                "find": "__MCR_REGISTRY__",
                "replaceWith": "$(MCRRegistry)"
              },
              {
                "find": "__DEV_MCR_AGENT_REPOSITORY__",
                "replaceWith": "$(DevMCRAgentRepository)"
              },
              {
                  "find": "__LINUX_TAG__",
                  "replaceWith": "$(LinuxTag)"
              },
              {
                  "find": "__WINDOWS_TAG__",
                  "replaceWith": "$(WindowsTag)"
              },
              {
                "find": "__PROD_MCR_AGENT_REPOSITORY__",
                "replaceWith": "$(ProdMCRAgentRepository)"
              },
              {
                "find": "__DEV_MCR_CHART_REPOSITORY__",
                "replaceWith": "$(DevMCRChartRepository)"
              },
              {
                "find": "__PROD_MCR_CHART_REPOSITORY__",
                "replaceWith": "$(ProdMCRChartRepository)"
              },
              {
                "find": "__CHART_TAG__",
                "replaceWith": "$(ChartTag)"
              }
          ]
      }                                                                                                                                                
  ]
}