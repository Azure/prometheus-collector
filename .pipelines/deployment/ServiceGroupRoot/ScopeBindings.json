        {
          "find": "__PROD_ACR_AGENT_REPOSITORY__",
          "replaceWith": "$config(prodACRAgentRepository)"
        },
        {
          "find": "__PROD_ACR_CHART_REPOSITORY__",
          "replaceWith": "$config(prodACRChartRepository)"
        },
        {
          "find": "__PROD_ACR_NE_REPOSITORY__",
          "replaceWith": "$config(prodACRNodeExporterRepository)"
        },
        {
