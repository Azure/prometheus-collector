        },
        {
          "find": "__MCR_REGISTRY__",
          "replaceWith": "$config(mcrRegistry)"
        },
        {
          "find": "__DEV_MCR_AGENT_REPOSITORY__",
          "replaceWith": "$config(devMCRAgentRepository)"
        },
        {
          "find": "__DEV_MCR_NE_REPOSITORY__",
