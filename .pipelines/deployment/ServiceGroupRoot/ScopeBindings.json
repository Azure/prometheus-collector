          "find": "__PUSH_NEW_NE_CHART__",
          "replaceWith": "$config(pushNewNEChart)"
        },
        {
          "find": "__NE_CHART_TAG__",
          "replaceWith": "$config(NEChartTag)"
        },
        {
          "find": "__ARC_HELM_CHART_NAME__",
          "replaceWith": "$config(arcHelmChartName)"
        },
        {
          "find": "__ONLY_CCP_RELEASE__",
          "replaceWith": "$config(onlyCCPRelease)"
        }
      ]
    }
  ]
}