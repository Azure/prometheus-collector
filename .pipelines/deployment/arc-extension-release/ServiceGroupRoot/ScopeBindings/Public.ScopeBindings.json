{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [       
      {
          "scopeTagName": "CanaryPreview",
          "bindings": [
              {
                  "find": "__RELEASE_STAGE__",
                  "replaceWith": "CanaryPreview"
              },
              {
                  "find": "__ADMIN_SUBSCRIPTION_ID__",
                  "replaceWith": "$(ADMIN_SUBSCRIPTION_ID)"
