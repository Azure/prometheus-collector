{
  "$schema": "http://schema.express.azure.com/schemas/2022-01-01/RolloutParameters.json",
  "contentVersion": "1.0.0.0",
  "shellExtensions": [
    {
      "name": "ArcExtensionRelease",
      "type": "ShellExtensionType",
      "properties": {
        "maxExecutionTime": "PT1H",
        "useFallBackLocations": false,
        "skipDeleteAfterExecution": false
      },
      "package": {
        "reference": {
          "path": "extension-artifacts.tar.gz"
        }
      },
      "launch": {
        "command": [
          "/bin/bash",
          "arcExtensionRelease.sh"
        ],
        "environmentVariables": [
          {
            "name": "LOCATION",
            "value": "__LOCATION__"
          },
          {
              "name": "EXTENSION_NAME",
              "value": "__EXTENSION_TYPE_NAME__"
          },
          {
              "name": "PACKAGE_CONFIG_NAME",
              "value": "__PACKAGE_CONFIG__"
          },
          {
              "name": "HELM_CHART_ENDPOINT",
              "value": "__HELM_CHART_ENDPOINT__"
          },
          {
              "name": "IS_CUSTOMER_HIDDEN",
              "value": "__IS_CUSTOMER_HIDDEN__"
          },
          {
              "name": "ARC_API_URL",
              "value": "__ARC_API_URL__"
          },
          {
              "name": "API_VERSION",
              "value": "__ARC_API_VERSION__"
          },
          {
              "name": "SUBSCRIPTION",
              "value": "__SUBSCRIPTION__"
          },
          {
              "name": "RELEASE_TRAIN",
              "value": "__RELEASE_TRAIN__"
          },
          {
              "name": "CHART_VERSION",
              "value": "__VERSION__"
          },
          {
            "name": "METHOD",
            "value": "__METHOD__"
          },
          {
            "name": "RESOURCE_AUDIENCE",
            "value": "__RESOURCE_AUDIENCE__"
          }
        ],
        "identity": {
          "type": "userAssigned",
          "userAssignedIdentities": [
            "__MSI__"
          ]
        }
      }
    }
  ],
  "wait": [
    {
      "name": "waitSdpBakeTime",
      "properties": {
        "duration": "PT24H"
      }
    }
  ]
}